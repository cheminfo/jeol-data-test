{
  "name": "jeol-data-test",
  "version": "0.2.3",
  "description": "provides JEOL NMR data files for testing purposes",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "data/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cheminfo/jeol-data-test.git"
  },
  "bugs": {
    "url": "https://github.com/cheminfo/jeol-data-test/issues"
  },
  "keywords": [
    "magnetic resonance",
    "nmr",
    "jeol",
    "dataset"
  ],
  "homepage": "https://github.com/cheminfo/jeol-data-test#readme",
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "rollup -c",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "jest --coverage"
  },
  "author": "Julien Wist",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "eslint": "^8.15.0",
    "eslint-config-cheminfo": "^8.0.2",
    "jest": "^28.1.3",
    "prettier": "^2.6.2",
    "rollup": "^2.72.1"
  },
  "dependencies": {
    "filelist-utils": "^0.5.0",
    "url": "^0.11.0"
  }
}
